{
  "name": "re-theme",
  "version": "1.1.0",
  "description": "Simple Theme switcher / builder for React and React Native",
  "main": "build/index.web.js",
  "repository": "https://github.com/lancetipton/ReTheme",
  "author": "Lance Tipton",
  "license": "MIT",
  "scripts": {
    "start": "yarn dev",
    "dev": "rollup -c rollup.config.js -w",
    "build": "rollup -c rollup.config.js",
    "test": "NODE_ENV=test jest --no-cache --verbose"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.6.3",
    "@rollup/plugin-alias": "2.2.0",
    "babel-jest": "24.9.0",
    "jest": "24.7.1",
    "jsutils": "git+https://github.com/lancetipton/jsutils.git",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "0.61.1",
    "react-native-web": "0.11.7",
    "rollup": "1.20.3",
    "rollup-jest": "0.0.2",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-cleanup": "3.1.1",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-terser": "5.1.2"
  },
  "peerDependencies": {
    "jsutils": "git+https://github.com/lancetipton/jsutils.git",
    "react": "16.9.0"
  },
  "optionalDependencies": {
    "react-dom": "16.9.0",
    "react-native": "0.61.1",
    "react-native-web": "0.11.7"
  },
  "jest": {
    "preset": "rollup-jest",
    "moduleNameMapper": {
      "ReDimensions": "<rootDir>/src/dimensions/dimensions.web.js",
      "RePlatform": "<rootDir>/src/context/platform.web.js"
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/index.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "es6"
    ],
    "globals": {
      "__DEV__": true
    },
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  }
}
